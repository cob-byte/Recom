<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/backbtn"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg"
    android:paddingLeft="20dp"
    android:paddingTop="10dp"
    android:paddingEnd="10dp"
    android:paddingRight="10dp"
    android:paddingBottom="10dp"
    tools:context=".Registration"
    tools:layout_editor_absoluteX="-32dp"
    tools:layout_editor_absoluteY="-16dp">

    <Button
        android:id="@+id/femalebtn"
        android:layout_width="85dp"
        android:layout_height="30dp"
        android:background="@drawable/editbox"
        android:backgroundTint="#FFFFFF"
        android:clickable="true"
        android:paddingTop="1dp"
        android:paddingBottom="1dp"
        android:text="Female"
        android:textSize="12sp"
        tools:layout_editor_absoluteX="292dp"
        tools:layout_editor_absoluteY="538dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background=""
        android:text="Gender"
        android:textColor="#FFFFFF"
        android:textSize="14sp"
        tools:layout_editor_absoluteX="227dp"
        tools:layout_editor_absoluteY="508dp" />

    <EditText
        android:id="@+id/dateDD"
        android:layout_width="57dp"
        android:layout_height="30dp"
        android:background="@drawable/editbox"
        android:backgroundTint="@color/colorPrimary"
        android:ems="10"
        android:hint="DD"
        android:inputType="date"
        android:paddingLeft="18dp"
        android:textSize="12sp"
        tools:layout_editor_absoluteX="69dp"
        tools:layout_editor_absoluteY="538dp" />

    <EditText
        android:id="@+id/dateYYY"
        android:layout_width="77dp"
        android:layout_height="30dp"
        android:background="@drawable/editbox"
        android:backgroundTint="@color/colorPrimary"
        android:ems="10"
        android:hint="YYYY"
        android:inputType="date"
        android:paddingLeft="20dp"
        android:textSize="12sp"
        tools:layout_editor_absoluteX="130dp"
        tools:layout_editor_absoluteY="538dp" />

    <EditText
        android:id="@+id/phonenum5"
        android:layout_width="279dp"
        android:layout_height="32dp"
        android:background="@drawable/editbox"
        android:backgroundTint="#FFFFFF"
        android:ems="10"
        android:hint="XXXX-XXXX-XXX"
        android:inputType="phone"
        android:paddingLeft="20dp"
        android:paddingTop="5dp"
        android:paddingEnd="10dp"
        android:paddingRight="10dp"
        android:paddingBottom="5dp"
        android:textSize="12sp"
        tools:layout_editor_absoluteX="91dp"
        tools:layout_editor_absoluteY="466dp" />

    <Button
        android:id="@+id/btn_signup"
        android:layout_width="96dp"
        android:layout_height="35dp"
        android:background="@drawable/editbox"
        android:text="Sign-Up"
        android:textSize="12sp"
        tools:layout_editor_absoluteX="146dp"
        tools:layout_editor_absoluteY="593dp" />

    <TextView
        android:id="@+id/RegistrationForm"
        android:layout_width="371dp"
        android:layout_height="157dp"
        android:text="Registration Form"
        android:textSize="50sp"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="41dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Register Now!"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="133dp"
        tools:layout_editor_absoluteY="170dp" />

    <EditText
        android:id="@+id/emailadd"
        android:layout_width="369dp"
        android:layout_height="33dp"
        android:background="@drawable/editbox"
        android:backgroundTint="#FFFFFF"
        android:ems="10"
        android:hint="Type your e-mail address"
        android:inputType="textEmailAddress"
        android:paddingLeft="20dp"
        android:paddingTop="10dp"
        android:paddingEnd="10dp"
        android:paddingRight="10dp"
        android:paddingBottom="10dp"
        android:textSize="12sp"
        tools:layout_editor_absoluteX="4dp"
        tools:layout_editor_absoluteY="396dp" />

    <EditText
        android:id="@+id/Lname"
        android:layout_width="375dp"
        android:layout_height="33dp"
        android:background="@drawable/editbox"
        android:backgroundTint="#FFFFFF"
        android:ems="10"
        android:hint="Last Name"
        android:inputType="textPersonName"
        android:paddingLeft="20dp"
        android:paddingTop="10dp"
        android:paddingEnd="10dp"
        android:paddingRight="10dp"
        android:paddingBottom="10dp"
        android:textSize="12sp"
        tools:layout_editor_absoluteX="3dp"
        tools:layout_editor_absoluteY="324dp" />

    <EditText
        android:id="@+id/Fname"
        android:layout_width="375dp"
        android:layout_height="33dp"
        android:background="@drawable/editbox"
        android:backgroundTint="#FFFFFF"
        android:ems="10"
        android:hint="First Name"
        android:inputType="textPersonName"
        android:paddingLeft="20dp"
        android:paddingTop="10dp"
        android:paddingEnd="10dp"
        android:paddingRight="10dp"
        android:paddingBottom="10dp"
        android:textSize="12sp"
        tools:layout_editor_absoluteX="3dp"
        tools:layout_editor_absoluteY="243dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background=""
        android:text="Phone Number"
        android:textColor="#FFFFFF"
        android:textSize="14sp"
        tools:layout_editor_absoluteX="5dp"
        tools:layout_editor_absoluteY="439dp" />

    <EditText
        android:id="@+id/phonenum"
        android:layout_width="74dp"
        android:layout_height="30dp"
        android:background="@drawable/editbox"
        android:backgroundTint="#FFFFFF"
        android:ems="10"
        android:hint="+63"
        android:inputType="phone"
        android:paddingLeft="25dp"
        android:paddingTop="5dp"
        android:paddingEnd="10dp"
        android:paddingRight="10dp"
        android:paddingBottom="5dp"
        android:textSize="12sp"
        tools:layout_editor_absoluteX="6dp"
        tools:layout_editor_absoluteY="466dp" />

    <Button
        android:id="@+id/button2"
        android:layout_width="71dp"
        android:layout_height="36dp"
        android:text="Back"
        android:textSize="12sp"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="4dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background=""
        android:text="E-mail Address"
        android:textColor="#FFFFFF"
        android:textSize="14sp"
        tools:layout_editor_absoluteX="4dp"
        tools:layout_editor_absoluteY="367dp" />

    <EditText
        android:id="@+id/Mname"
        android:layout_width="375dp"
        android:layout_height="33dp"
        android:background="@drawable/editbox"
        android:backgroundTint="#FFFFFF"
        android:ems="10"
        android:hint="Middle Name"
        android:inputType="textPersonName"
        android:paddingLeft="20dp"
        android:paddingTop="10dp"
        android:paddingEnd="10dp"
        android:paddingRight="10dp"
        android:paddingBottom="10dp"
        android:textSize="12sp"
        tools:layout_editor_absoluteX="3dp"
        tools:layout_editor_absoluteY="284dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background=""
        android:text="Date of Birth"
        android:textColor="#FFFFFF"
        android:textSize="14sp"
        tools:layout_editor_absoluteX="6dp"
        tools:layout_editor_absoluteY="509dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Name"
        android:textColor="#FFFFFF"
        android:textSize="14sp"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="215dp" />

    <EditText
        android:id="@+id/dateMM"
        android:layout_width="57dp"
        android:layout_height="30dp"
        android:background="@drawable/editbox"
        android:backgroundTint="@color/colorPrimary"
        android:ems="10"
        android:hint="MM"
        android:inputType="date"
        android:paddingLeft="18dp"
        android:textSize="12sp"
        tools:layout_editor_absoluteX="6dp"
        tools:layout_editor_absoluteY="538dp" />

    <Button
        android:id="@+id/malebtn"
        android:layout_width="75dp"
        android:layout_height="30dp"
        android:background="@drawable/editbox"
        android:backgroundTint="#FFFFFF"
        android:clickable="true"
        android:cursorVisible="true"
        android:paddingTop="1dp"
        android:paddingBottom="1dp"
        android:text="Male"
        android:textSize="12sp"
        tools:layout_editor_absoluteX="217dp"
        tools:layout_editor_absoluteY="538dp" />

</androidx.constraintlayout.widget.ConstraintLayout>